{
    "manifest_version": 3,
    "name": "Tab Finder",
    "version": "1.1",
    "description": "Find tabs easily.",
    "permissions": ["tabs", "tabGroups"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}
